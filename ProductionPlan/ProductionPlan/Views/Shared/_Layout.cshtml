<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Dashboard</title>
    <link href="~/Content/Css/Global.css" rel="stylesheet" />
    <link href="/Content/Bootstrap/Css/bootstrap.min.css" rel="stylesheet" />
    <script src="/Content/Js/jquery-1.10.2.min.js"></script>
    <script src="/Content/Js/global.js"></script>
    <link href="~/Content/Css/NewDashboard.css" rel="stylesheet" />
    <link href="~/Content/Css/Common.css" rel="stylesheet" />
    @{
        ProductionPlan.Areas.User.DAL.LogIn_Result currentModel = Model;
        if (currentModel == null)
        {
            currentModel = new ProductionPlan.Areas.User.DAL.LogIn_Result();
        }
    }
</head>
<body class="no-skin">
    <div class="main_body">
        <div id="navbar" class="navbar navbar-default">
            <div class="navbar-container nav_head" id="navbar-container">
                <!-- #section:basics/sidebar.mobile.toggle -->
                <button type="button" class="navbar-toggle pull-left" data-toggle="collapse" id="menu-toggler">
                    <div class="glyphicon glyphicon-align-justify"></div>
                </button>
                <!-- /section:basics/sidebar.mobile.toggle -->
                <div class="navbar-header pull-left" id="nav_left">
                    <div id="" style="width: 60px;"></div>
                </div>
                <div class="navbar-header pull-left" id="nav_left">
                    <div id="go_app_text" style="background-image: none;"><span class="small_font">Population Management</span></div>
                </div>
                <div class="navbar-buttons navbar-header pull-left" role="navigation">
                    <ul id="access_menu_container" class="nav ace-nav"></ul>
                </div>
                <!-- #section:basics/navbar.dropdown -->
                <div class="navbar-buttons navbar-header pull-right" role="navigation">
                    <ul id="notifications_container" class="nav ace-nav">
                        <li class="light-blue">
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                                <span class="user_image_container"><img src="/Content/Images/default-man.png"></span>
                                <span>@Model.Name</span> <b class="caret"></b>
                            </a>
                            <!--Notification Dropdown-->
                            <ul class="pull-right dropdown-menu dropdown-arrow">
                                <li class="dropdown-header" role="presentation">
                                    <span>You are login as</span>
                                    <span>@Model.Name</span>
                                </li>
                                <li>
                                    <a id="btn_change_password" style="width:100%">
                                        <div class="clearfix">
                                            <div class="notification-icon pull-left">
                                                <i class="fa fa-key"></i>
                                            </div>
                                            <div class="notification-body pull-right">
                                                <span class="description">Change Password</span>
                                            </div>
                                        </div>
                                    </a>
                                </li>
                                <li>
                                    <a href="/" style="width:100%">
                                        <div class="clearfix">
                                            <div class="notification-icon pull-left">
                                                <i class="fa fa-user"></i>
                                            </div>
                                            <div class="notification-body pull-right">
                                                <span class="description">My Account</span>
                                            </div>
                                        </div>
                                    </a>
                                </li>
                                <li>
                                    <a href="/User/Account/LogOff/" style="width:100%">
                                        <div class="clearfix">
                                            <div class="notification-icon pull-left">
                                                <i class="fa fa-power-off"></i>
                                            </div>
                                            <div class="notification-body pull-right">
                                                <span class="description">Logout</span>
                                            </div>
                                        </div>
                                    </a>
                                </li>
                            </ul>
                        </li>
                    </ul>
                </div>
                <!-- /section:basics/navbar.dropdown -->
            </div>
        </div>
        <div class="main-container" id="main-container">
            <div class="main-content page-content-wrapper">
                <div id="page_content" class="mid_body page-content">
                    @RenderBody()
                </div>
            </div>
            <link href="/Content/Css/Dashboard.css" rel="stylesheet" />
        </div><!-- /.main-container -->
        <div class="footer">
            <p> &copy; 2015.</p>
        </div>
        @RenderSection("scripts", required: false)
        <script src="/Content/Bootstrap/Js/bootstrap.min.js"></script>
        <link href="/Content/Awesome/css/font-awesome.min.css" rel="stylesheet" />
    </div>
</body>
</html>







